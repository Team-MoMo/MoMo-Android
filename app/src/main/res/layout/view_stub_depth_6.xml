<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_rectangle_depth6">

    <ImageView
        android:id="@+id/imageView_seaweed"
        android:layout_width="104dp"
        android:layout_height="96dp"
        android:src="@drawable/object_depth6_seaweed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_shark"
        android:layout_width="274dp"
        android:layout_height="206dp"
        android:layout_marginTop="124dp"
        android:layout_marginBottom="42dp"
        android:src="@drawable/object_depth6_shark"
        app:layout_constraintBottom_toTopOf="@+id/imageView_seaweed_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_rock"
        android:layout_width="90dp"
        android:layout_height="340dp"
        android:src="@drawable/object_depth6_rock"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView_seaweed_2"
        android:layout_width="192dp"
        android:layout_height="190dp"
        android:src="@drawable/object_depth6_seaweed_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>