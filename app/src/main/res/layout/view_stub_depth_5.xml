<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_rectangle_depth5">

    <ImageView
        android:id="@+id/imageView_seaweed"
        android:layout_width="130dp"
        android:layout_height="154dp"
        android:src="@drawable/object_depth5_seaweed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_rock"
        android:layout_width="128dp"
        android:layout_height="186dp"
        android:src="@drawable/object_depth5_rock"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_whale"
        android:layout_width="314dp"
        android:layout_height="318dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/object_depth5_whale"
        app:layout_constraintBottom_toTopOf="@+id/imageView_fishes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_seaweed_2"
        android:layout_width="104dp"
        android:layout_height="180dp"
        android:src="@drawable/object_depth5_seaweed_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView_fishes"
        android:layout_width="170dp"
        android:layout_height="226dp"
        android:src="@drawable/object_depth5_fishes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>